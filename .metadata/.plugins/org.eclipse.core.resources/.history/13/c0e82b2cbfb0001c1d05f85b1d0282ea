#include "qbuffer.h"

void qbufferInit(void)
{

}

bool qbufferCreate(qbuffer_t *p_node, uint8_t *p_buf, uint32_t length)
{
  bool ret = true;

  p_node->in    = 0;
  p_node->out   = 0;
  p_node->len   = length;
  p_node->p_buf = p_buf;

  if (p_node == NULL)
  {
    p_node->len = 0;
    p_node->p_buf = NULL;
    ret = false;
  }

  return ret;

}

